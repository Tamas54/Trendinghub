# TrendMaster Environment Variables

# YouTube Data API v3 Key
YOUTUBE_API_KEY=

# OpenAI API Key
OPENAI_API_KEY=

# Google AI API Keys
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=

# Google AI Model Configuration
# Gemini 3 - Text generation
GEMINI_TEXT_MODEL=
# Nano Banana Pro (Gemini 3 Pro Image) - Image generation
GEMINI_IMAGE_MODEL=
# Veo 3.1 - Video generation
GEMINI_VIDEO_MODEL=

# AI Provider Selection
# Options: 'openai' or 'google'
# NOTE: Using OpenAI for text/image, Google Veo 3.1 for video (Sora requires verified organization)
AI_PROVIDER=

# Flask Configuration
PORT=
SECRET_KEY=
FLASK_DEBUG=
